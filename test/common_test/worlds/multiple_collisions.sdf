<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">

            <plugin
      filename="gz-sim-physics-system"
      name="gz::sim::systems::Physics">
        <engine><filename>gz-physics-bullet-featherstone-plugin</filename></engine>
    </plugin>
    <plugin
      filename="gz-sim-scene-broadcaster-system"
      name="gz::sim::systems::SceneBroadcaster">
    </plugin>

    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <emissive>0.8 0.8 0.8 1</emissive>
          </material>
        </visual>
      </link>
    </model>

    <model name="box">
      <static>false</static>
      <pose>0 0 4.0 0 0 0</pose>
      <link name="box_link">
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <collision name="box_collision_upper">
          <pose>0 0 0.25 0 0 0</pose>
          <geometry>
            <box>
              <size>1 1 0.5</size>
            </box>
          </geometry>
        </collision>
        <visual name="box_visual_upper">
          <pose>0 0 0.25 0 0 0</pose>
          <geometry>
            <box>
              <size>1 1 0.5</size>
            </box>
          </geometry>
        </visual>


        <collision name="box_collision_lower">
          <pose>0 0 -0.25 0 0 0</pose>
          <geometry>
            <box>
              <size>1 1 0.5</size>
            </box>
          </geometry>
        </collision>
        <visual name="box_visual_lower">
          <pose>0 0 -0.25 0 0 0</pose>
          <geometry>
            <box>
              <size>1 1 0.5</size>
            </box>
          </geometry>
        </visual>
      </link>
    </model>
  </world>
</sdf>
